{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SQLite"
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "MacroErrors": "Throw"
      },
      "Unattended": {
        "UnattendedUserEmail": "admin@admin.com",
        "UnattendedUserName": "admin",
        "UnattendedUserPassword": "0123456789"
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto",
        "ModelsDirectory": "~/Models"
      },
      "Hosting": {
        "Debug": true
      },
      "RuntimeMinification": {
        "UseInMemoryCache": true,
        "CacheBuster": "Timestamp"
      }
    }
  },
  "uSync": {
    "Settings": {
      "ImportOnFirstBoot": true
    }
  },

  "FeatureManagement": {
    "SearchImages": false,
    "BetaTemplate": true,
    "RestrictPublish": {
      "EnabledFor": [
        {
          "Name": "SendingContentNotification",
          "Parameters": {
            "DocumentTypes": [ "content" ]
          }
        }
      ]
    },
    "HideURLsAndPreview": {
      "EnabledFor": [
        {
          "Name": "SendingContentNotification",
          "Parameters": {
            "DocumentTypes": [ "content" ]
          }
        }
      ]
    },
    "BetaUser": {
      "EnabledFor": [
        {
          "Name": "Microsoft.Targeting",
          "Parameters": {
            "Audience": {
              "Users": [],
              "Groups": [
                {
                  "Name": "Beta User",
                  "RolloutPercentage": 100
                }
              ],
              "DefaultRolloutPercentage": 0,
              "Exclusion": {
                "Users": [],
                "Groups": [
                  "Ring2"
                ]
              }
            }
          }
        }
      ]
    },
    "BackOfficeUser": {
      "EnabledFor": [
        {
          "Name": "UmbracoBackOfficeUser",
          "Parameters": {
            "Match": "Any",
            "Groups": {
              "Match": "Any",
              "Values": []
            },
            "EmailAddresses": {
              "Match": "Any",
              "Values": []
            }
          }
        }
      ]
    },
    "DanishSite": {
      "EnabledFor": [
        {
          "Name": "UmbracoDomain",
          "Parameters": {
            "Domains": [
              "/da"
            ]
          }
        }
      ]
    }
  }
}